{
    "manifest_version": 3,
    "name": "sipicey",
    "version": "1.2",
    "description": "A simple Webrtc Sip client",
    "icons": {
        "16": "images/16x16.png",
        "32": "images/32x32.png",
        "64": "images/64x64.png",
        "128":"images/128x128.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "options.html",
    "permissions":[
        "contextMenus"
    ]
  }